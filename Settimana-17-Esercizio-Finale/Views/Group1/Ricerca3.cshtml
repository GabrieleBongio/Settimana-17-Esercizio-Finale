@model List<Settimana_17_Esercizio_Finale.Models.Group3>
@{
    ViewBag.Title = "Ricerca3";
}

<h2>Violazioni con più di 10 punti decurtati</h2>

<div>
    @foreach (Settimana_17_Esercizio_Finale.Models.Group3 g in Model)
    {
        <div class="border-top border-2 px-3">
            <p class="mt-3 m-0">Nome Cognome: @g.Nome @g.Cognome </p>
            <p class="m-0">Data Violazione: @g.dataViolazione </p>
            <p class="m-0">Importo: @g.Importo </p>
            <p class="mb-3">Punti Decurtati: @g.DecurtamentoPunti</p>

        </div>
    }
</div>

